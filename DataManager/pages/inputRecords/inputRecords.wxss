.main{
  display: flex;
  flex-direction: column;
  width: 100%;
  height: 100%;
  background-color: white
}
.title{
  display: flex;
  flex-direction: column;
  width: 100%-10px;
  height: 40px;
  margin: 5px;
  background-color: white;
}

.title-content{
  display: flex;
  height: 40px;
  margin-top: 15px;
  margin-left: 5px;
  margin-right: 5px;
  justify-content: center;
  align-content: center;
  text-align: center;
  font-size: 35rpx;
}

.line{
  background: lightskyblue;
  height: 10rpx;
  margin-top: 15px;
  margin-bottom: 15px;
  margin-left: 30px;
  margin-right: 30px;
  align-items: center;
  justify-content: center;
}

.list{
  display: flex;
  flex-direction: column;
  width: 100%;
  height: 100%;
}

.list-item{
  display: flex;
  flex-direction: row;
  height: 50px;
  background-color: white;
  font-size: 35rpx;
}

.label-content{
  display: flex;
  margin-left: 15px;
  height: 35px;
  width: 60px;
  margin-top: 5px;
}

.item-content{
  display: flex;
  height: 35px;
  margin-bottom: 10px;
  text-align: center;
  justify-content: center;
  background-color: whitesmoke
}

.confirmBtn{
  display: flex;
  margin-left: 10px;
  margin-right: 10px;
  height:40px;
  text-align: center;
  justify-content: center;
  font-size: 35rpx;
}

.table {
  border: 0px solid darkgray;
  width: 100%-20px;
  margin-left: 10px;
  margin-right: 10px;

}
.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 3rem;
  align-items: center;
  font-size: 30rpx;
}
.td {
    width:50%;
    justify-content: center;
    text-align: center;
    font-size: 30rpx;
}
.bg-w{
  background: snow;
}
.bg-g{
  background: #E6F3F9;
}
.th {
  width: 50%;
  justify-content: center;
  background: #3366FF;
  color: #fff;
  display: flex;
  height: 3rem;
  align-items: center;
  font-size: 30rpx;
}
